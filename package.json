{
  "name": "vite-plugin-fastify-routes",
  "version": "1.1.3",
  "description": "File-based routing for Fastify applications using Vite.",
  "main": "./dist/vite-plugin-fastify-routes.cjs",
  "module": "./dist/vite-plugin-fastify-routes.js",
  "types": "./dist/src/vite-plugin-fastify-routes.d.ts",
  "type": "module",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/vite-plugin-fastify-routes.d.ts",
        "default": "./dist/vite-plugin-fastify-routes.js"
      },
      "require": {
        "types": "./dist/src/vite-plugin-fastify-routes.d.ts",
        "default": "./dist/vite-plugin-fastify-routes.cjs"
      }
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "unit": "vitest --run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vanilla-IceCream/vite-plugin-fastify-routes.git"
  },
  "keywords": [
    "vite",
    "fastify",
    "router"
  ],
  "author": "Vanilla IceCream",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vanilla-IceCream/vite-plugin-fastify-routes/issues"
  },
  "homepage": "https://github.com/Vanilla-IceCream/vite-plugin-fastify-routes#readme",
  "dependencies": {
    "glob": "^11.0.3",
    "lodash.maxby": "^4.6.0"
  },
  "devDependencies": {
    "@types/lodash.maxby": "^4.6.9",
    "@types/node": "^22.15.31",
    "@vitest/coverage-v8": "^3.2.3",
    "fastify": "^5.4.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.3"
  }
}
